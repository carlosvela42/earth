.table_form {
  width: 100%;
}
.table_form tr {
  height: 48px;
  border-bottom: 1px solid #e4e6e8;
}
.table_form tr td:nth-child(1) {
  font-weight: bold;
}
.table_form tr td {
  vertical-align: middle;
}

.table_form_login {
  width: 100%;
  text-align: center;
  vertical-align: middle;
}
.table_form_login tr:first-child {
  height: 85px;
}
.table_form_login tr {
  height: 45px;
}
.table_form_login tr td:nth-child(1) {
  font-weight: bold;
}
.table_form_login tr td {
  vertical-align: middle;
}

.div_login {
  width: 350px;
  height: 310px;
  margin: auto;
  padding-top: 15%;
}

.region {
  padding: 10px 0;
}

.debug_info {
  position: fixed;
  bottom: 0px;
  height: 20px;
  padding-left: 10px;
  z-index: 1000;
}

